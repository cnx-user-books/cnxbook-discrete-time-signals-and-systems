    <entry><m:math><m:mrow><m:mrow class="MJX-TeXAtom-ORD"><m:mi mathvariant="normal">I</m:mi><m:mi mathvariant="normal">m</m:mi><m:mtext> </m:mtext></m:mrow><m:mo stretchy="false">(</m:mo><m:mi>X</m:mi><m:mo stretchy="false">[</m:mo><m:mi>k</m:mi><m:mo stretchy="false">]</m:mo><m:mo stretchy="false">)</m:mo></m:mrow></m:math></entry>
    <entry><m:math><m:mrow><m:mrow class="MJX-TeXAtom-ORD"><m:mo stretchy="false">|</m:mo></m:mrow><m:mi>X</m:mi><m:mo stretchy="false">[</m:mo><m:mi>k</m:mi><m:mo stretchy="false">]</m:mo><m:mrow class="MJX-TeXAtom-ORD"><m:mo stretchy="false">|</m:mo></m:mrow></m:mrow></m:math></entry>
    <entry><m:math><m:mrow><m:mi mathvariant="normal">∠</m:mi><m:mtext> </m:mtext><m:mi>X</m:mi><m:mo stretchy="false">[</m:mo><m:mi>k</m:mi><m:mo stretchy="false">]</m:mo></m:mrow></m:math></entry>
